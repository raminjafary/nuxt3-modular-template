{
	"name": "nuxt3",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"prepare": "husky install",
		"dev": "nuxi dev",
		"build": "cross-env NODE_ENV=production nuxi build",
		"start": "node .output/server/index.mjs",
		"lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
		"lint:fix": "yarn lint --fix",
		"format:check": "prettier --ignore-path .gitignore .",
		"format:fix": "prettier  --ignore-path .gitignore -u --write .",
		"lint:style": "stylelint --ignore-path .gitignore '**/*.{scss,css,vue}'",
		"lint:style:fix": "yarn lint:style --fix",
		"generate:changelog": "standard-version --skip.commit --skip.tag",
		"release": "standard-version && git push origin master --follow-tags",
		"merge": "node scripts/create-merge-request.js",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.20.2",
		"@commitlint/cli": "^17.3.0",
		"@commitlint/config-conventional": "^17.3.0",
		"@intlify/nuxt3": "^0.2.4",
		"@types/jest": "^29.2.4",
		"@types/node": "^18.11.11",
		"@typescript-eslint/eslint-plugin": "^5.45.1",
		"@typescript-eslint/parser": "^5.45.1",
		"@vue/eslint-config-typescript": "^11.0.2",
		"@vue/test-utils": "^2.2.6",
		"autoprefixer": "^10.4.13",
		"babel-jest": "^29.3.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.29.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-nuxt": "^4.0.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.2",
		"jest": "^29.3.1",
		"lint-staged": "^13.1.0",
		"nuxt": "^3.0.0",
		"postcss": "^8.4.19",
		"postcss-html": "^1.5.0",
		"prettier": "^2.8.1",
		"standard-version": "^9.5.0",
		"stylelint": "^14.16.0",
		"stylelint-config-recommended": "^9.0.0",
		"stylelint-config-standard": "^29.0.0",
		"tailwindcss": "^3.2.4",
		"ts-jest": "^29.0.3",
		"typescript": "^4.9.4",
		"vite-plugin-eslint": "^1.8.1",
		"vue3-jest": "^27.0.0-alpha.1"
	},
	"dependencies": {
		"axios": "^1.2.1"
	}
}
