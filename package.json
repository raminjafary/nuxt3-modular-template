{
	"name": "nuxt3",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"prepare": "husky install",
		"dev": "nuxi dev",
		"build": "cross-env NODE_ENV=production nuxi build",
		"start": "node .output/server/index.mjs",
		"lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
		"lint:fix": "yarn lint --fix",
		"format:check": "prettier --ignore-path .gitignore .",
		"format:fix": "prettier  --ignore-path .gitignore -u --write .",
		"lint:style": "stylelint --ignore-path .gitignore '**/*.{scss,css,vue}'",
		"lint:style:fix": "yarn lint:style --fix",
		"generate:changelog": "standard-version --skip.commit --skip.tag",
		"release": "standard-version && git push origin master --follow-tags",
		"merge": "node scripts/create-merge-request.js",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.15.8",
		"@commitlint/cli": "^15.0.0",
		"@commitlint/config-conventional": "^15.0.0",
		"@intlify/nuxt3": "^0.1.9",
		"@types/jest": "^27.0.3",
		"@types/node": "^16.10.9",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"@vue/eslint-config-typescript": "^9.1.0",
		"@vue/test-utils": "^2.0.0-rc.15",
		"autoprefixer": "^10.3.7",
		"babel-jest": "^27.2.5",
		"cross-env": "^7.0.3",
		"eslint": "^8.3.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-nuxt": "^3.0.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.2",
		"jest": "^27.2.5",
		"lint-staged": "^12.1.2",
		"nuxt3": "3.0.0-27296423.f3082ca",
		"postcss": "^8.3.9",
		"prettier": "^2.5.0",
		"standard-version": "^9.3.2",
		"stylelint": "^14.1.0",
		"stylelint-config-recommended": "^6.0.0",
		"stylelint-config-standard": "^24.0.0",
		"tailwindcss": "^2.2.17",
		"ts-jest": "^27.0.5",
		"typescript": "^4.5.2",
		"vite-plugin-eslint": "^1.3.0",
		"vue3-jest": "^27.0.0-alpha.1"
	},
	"dependencies": {
		"axios": "^0.24.0"
	}
}
